{
  "name": "goal-model-syntactic-analyzer",
  "version": "1.0.0",
  "description": "",
  "bin": "build/index.js",
  "scripts": {
    "start": "",
    "dev": "nodemon --watch './**/*.ts' --exec 'clear;ts-node' core/index.ts",
    "build": "rm -rf ./build && tsc --build",
    "build:run": "rm -rf ./build && tsc --build && node ./build/index.js",
    "pkg": "npm run build && pkg package.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^16.6.1",
    "typescript": "^4.3.5",
    "ts-node": "^10.2.0",
    "nodemon": "^2.0.12",
    "pkg": "^5.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablomuro/goal-model-syntactic-analyzer.git"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "goal-model-examples/**/*",
    "targets": [
      "node14-linux-x64"
    ],
    "outputPath": "dist"
  },
  "author": "Pablo Alves Muro Martinez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pablomuro/goal-model-syntactic-analyzer/issues"
  },
  "homepage": "https://github.com/pablomuro/goal-model-syntactic-analyzer#readme"
}
